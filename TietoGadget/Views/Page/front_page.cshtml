@model DD4T.ContentModel.IPage
@using DD4T.Mvc.Html
@using TietoGadget.Helpers;
@*
    This is a very basic Page View, you may delete this if you wish.
*@
@section Title
    {
    @Model.Title
}
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    Session["pageuri"] = Model.Id;
}

@Html.RenderComponentPresentationsByView("homecarousel")
<div class="row">
    @Html.RenderComponentPresentationsByView("ST_promotion")
</div>
<div class="row">
    @Html.RenderComponentPresentationsByView("ugcinterface")
</div>
<div class="row">
    @Html.RenderComponentPresentationsByView("mostviewedcontent")
</div>

@Html.SiteEditPage(Model)